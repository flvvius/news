{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", ".output/**", ".vercel/output/**", "_generated/**"],
			"env": [
				"VITE_CONVEX_URL",
				"VITE_CONVEX_SITE_URL",
				"EXPO_PUBLIC_CONVEX_SITE_URL",
				"CONVEX_URL",
				"CONVEX_SITE_URL",
				"CONVEX_DEPLOYMENT",
				"CONVEX_DEPLOY_KEY",
				"CONVEX_ACCESS_TOKEN",
				"BETTER_AUTH_SECRET",
				"GOOGLE_CLIENT_ID",
				"GOOGLE_CLIENT_SECRET",
				"SITE_URL",
				"NATIVE_APP_URL"
			]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"dev:setup": {
			"cache": false,
			"persistent": true
		}
	}
}
